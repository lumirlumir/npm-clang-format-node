{
  "heading-style": {
    "style": "atx"
  },
  "ul-style": {
    "style": "dash"
  },
  "line-length": false,
  "commands-show-output": false,
  "no-duplicate-heading": {
    "siblings_only": true
  },
  "ol-prefix": {
    "style": "one"
  },
  "no-inline-html": {
    "allowed_elements": [
      "small",
      "sub",
      "sup",
      "u",
      "br",
      "details",
      "summary",
      "image",
      "code",
      "kbd",
      "mark",
      "ruby",
      "rp",
      "rt",
      "Badge"
    ]
  },
  "hr-style": {
    "style": "---"
  },
  "code-block-style": {
    "style": "fenced"
  },
  "code-fence-style": {
    "style": "backtick"
  },
  "emphasis-style": {
    "style": "asterisk"
  },
  "strong-style": {
    "style": "asterisk"
  },
  "table-column-count": false
}
